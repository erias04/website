{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "generators": {
    "@nx/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nx/react": {
      "application": {
        "babel": true
      }
    }
  },
  "defaultProject": "home",
  "nxCloudAccessToken": "Y2Y2YzQ5ODItMzMxNC00NDJiLTllNDktZWYzMzMwM2I4ZDhhfHJlYWQtd3JpdGU=",
  "targetDefaults": {
    "build": {
      "cache": true
    },
    "e2e": {
      "cache": true
    },
    "@nx/jest:jest": {
      "cache": true,
      "inputs": [
        "default",
        "^default"
      ],
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/tools/eslint-rules/**/*"
      ]
    }
  },
  "useInferencePlugins": false,
  "defaultBase": "main"
}
